{
  "name": "template",
  "version": "0.1.0",
  "description": "Template for creating node apps written in TS",
  "scripts": {
    "test": "jest",
    "cdk": "cdk",
    "start": "nodemon",
    "build:lambda": "esbuild ./src/lambda.ts --loader:.html=text --bundle --minify --outdir=dist --platform=node",
    "build:start": "esbuild ./src/start.ts --loader:.html=text --bundle --minify --outdir=dist --platform=node",
    "infra:deploy": "cdk deploy --force --require-approval never",
    "infra:destroy": "cdk destroy",
    "infra:diff": "cdk diff"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@swc/core": "^1.3.56",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "babel-jest": "^29.5.0",
    "esbuild": "^0.17.18",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.10.1",
    "aws-cdk": "^2.78.0",
    "aws-cdk-lib": "^2.78.0",
    "express": "^4.18.2"
  }
}
