{
  "name": "template",
  "version": "0.1.0",
  "description": "Template for creating node apps written in TS",
  "scripts": {
    "test": "jest",
    "cdk": "cdk",
    "start": "nodemon",
    "build:lambda": "ts-node ./infra/build.ts --target lambda",
    "build:server": "ts-node ./infra/build.ts --target server",
    "build:client": "ts-node ./infra/bundle.ts",
    "infra:deploy": "npm run build:client && cdk deploy --force --require-approval never",
    "infra:destroy": "cdk destroy",
    "infra:diff": "cdk diff"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@swc/core": "^1.3.56",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@vendia/serverless-express": "^4.10.1",
    "aws-cdk": "^2.78.0",
    "aws-cdk-lib": "^2.78.0",
    "babel-jest": "^29.5.0",
    "commander": "^10.0.1",
    "esbuild": "^0.17.18",
    "express": "^4.18.2",
    "htm": "^3.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "preact": "^10.13.2",
    "preact-render-to-string": "^6.0.3",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.328.0"
  }
}
